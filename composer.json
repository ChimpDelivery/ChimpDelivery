{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1.9",
        "barryvdh/laravel-debugbar": "^3.6",
        "biscolab/laravel-recaptcha": "^6.0",
        "doctrine/dbal": "^3.3",
        "encodia/laravel-health-env-vars": "^1.2",
        "fakerphp/faker": "^1.9.1",
        "firebase/php-jwt": "^6.0",
        "graham-campbell/github": "^11.0",
        "guzzlehttp/guzzle": "^7.4",
        "http-interop/http-factory-guzzle": "^1.0",
        "jantinnerezo/livewire-alert": "^2.2",
        "laravel/framework": "^9.0",
        "laravel/horizon": "^5.11",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.6",
        "laravel/telescope": "^4.13",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/flysystem-ftp": "^3.0",
        "livewire/livewire": "^2.11",
        "lorisleiva/laravel-actions": "^2.5",
        "predis/predis": "^2.0",
        "sentry/sentry-laravel": "^3.2",
        "spatie/cpu-load-health-check": "^1.0",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-ciphersweet": "^1.0",
        "spatie/laravel-cookie-consent": "^3.2",
        "spatie/laravel-health": "^1.8",
        "spatie/laravel-honeypot": "^4.3",
        "spatie/laravel-image-optimizer": "^1.7",
        "spatie/laravel-permission": "^5.5"
    },
    "require-dev": {
        "deployer/deployer": "^7.0",
        "laravel/breeze": "^1.8",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.0",
        "phpunit/phpunit": "^10.0.1",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "deploy-unlock": "php vendor/bin/dep deploy:unlock",
        "dashboard-down": "php artisan down --secret=\"262626\"",
        "dashboard-up": "php artisan up",
        "dashboard-static-analysis": "phpstan analyse --ansi --memory-limit 512M --generate-baseline"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
