{
    "name": "chimpdelivery/chimpdelivery",
    "type": "project",
    "description": "Designed to automate and monitor the CI/CD Steps for applications built with the Unity3D.",
    "keywords": [
        "framework",
        "laravel",
        "unity3d",
        "ci/cd",
        "appstoreconnect",
        "github",
        "jenkins"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1.9",
        "barryvdh/laravel-debugbar": "^3.6",
        "biscolab/laravel-recaptcha": "^6.0",
        "doctrine/dbal": "^3.3",
        "encodia/laravel-health-env-vars": "^1.5",
        "fakerphp/faker": "^1.21",
        "firebase/php-jwt": "^6.4",
        "graham-campbell/github": "^12.0",
        "guzzlehttp/guzzle": "^7.5",
        "hi-folks/lara-lens": "^0.4.0",
        "http-interop/http-factory-guzzle": "^1.0",
        "jantinnerezo/livewire-alert": "^2.2",
        "laravel/framework": "^10.0",
        "laravel/horizon": "^5.11",
        "laravel/pennant": "^1.2",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.6",
        "laravel/telescope": "^4.13",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/flysystem-ftp": "^3.0",
        "livewire/livewire": "^2.11",
        "lorisleiva/laravel-actions": "^2.5",
        "monicahq/laravel-cloudflare": "^3.5",
        "opcodesio/log-viewer": "^2.3",
        "predis/predis": "^2.0",
        "sentry/sentry-laravel": "^3.3",
        "spatie/cpu-load-health-check": "^1.0",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-ciphersweet": "^1.0",
        "spatie/laravel-cookie-consent": "^3.2",
        "spatie/laravel-health": "^1.8",
        "spatie/laravel-honeypot": "^4.3",
        "spatie/laravel-image-optimizer": "^1.7",
        "spatie/laravel-permission": "^5.9",
        "spatie/shiki-php": "^1.3"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "deployer/deployer": "^7.0",
        "laravel/breeze": "^1.8",
        "laravel/pint": "^1.7",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "deploy-unlock": "php vendor/bin/dep deploy:unlock",
        "dashboard-down": "php artisan down --secret=\"2626\"",
        "dashboard-up": "php artisan up",
        "dashboard-pint": "php vendor/bin/pint -v"
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-debugbar",
                "barryvdh/laravel-ide-helper"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
